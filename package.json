{
  "lint-staged": {
    "app/**/*.{js,jsx,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{package.json,.eslintrc.js,.prettierrc.js}": [
      "prettier --write",
      "eslint --fix"
    ],
    ".eslint-rules/**/*": [
      "prettier --write",
      "eslint --fix"
    ],
    "./**/*.rb": [
      "bundle exec rubocop -a --force-exclusion"
    ],
    "{Gemfile,Rakefile,config.ru}": [
      "bundle exec rubocop -a --force-exclusion"
    ],
    "app/views/**/*.html.erb": [
      "bundle exec erblint --lint-all --format compact -a"
    ]
  },
  "name": "app",
  "private": true,
